	     font="SansSerifSmall" font-style="UNDERLINE" h_pad="0" halign="right" height="16"
	     left="-210" mouse_opaque="true" name="create_new_account_text" v_pad="0" width="200"
	     hover="true" hover_color="50 115 185">
